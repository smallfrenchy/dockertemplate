<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>false</Beta>
  <Category>HomeAutomation:Network:Management</Category>
  <Date>2017-02-11</Date>
  <Changes></Changes>
  <Name>Sarah</Name>
  <Support></Support>
  <Repository>skydiscover/sarah-server-nodejs</Repository>
  <Project></Project>
  <Branch></Branch>
  <Registry>https://hub.docker.com/r/skydiscover/sarah-server-nodejs/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Overview>A personal HTTP bridge, take some request on the left side and call on the right side</Overview>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Icon></Icon>
  <Description></Description>
  <Environment>
    <Variable>
      <Name>TZ</Name>
      <Value>Europe/Paris</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/home/pi/SARAH-Server-NodeJS/plugins</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
</Container>
